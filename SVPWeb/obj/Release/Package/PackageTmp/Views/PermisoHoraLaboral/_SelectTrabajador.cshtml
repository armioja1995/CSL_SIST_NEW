@model SVPDomain.Trabajador
<div class="clearfix" style="margin-bottom: 0px;"></div>
<div class="form-group">
    <label class="col-sm-2 control-label">Nombres :</label>
    <div class="col-md-5">
        @Html.EditorFor(model => model.NombreCompleto, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
        @Html.ValidationMessageFor(model => model.NombreCompleto, "", new { @class = "text-danger" })
    </div>
</div>
<div class="clearfix" style="margin-bottom: 10px;"></div>
<div class="form-group">
    <label class="col-sm-2 control-label">Cargo :</label>
    <div class="col-md-5">
        @Html.EditorFor(model => model.Cargo.NombreCargo, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
        @Html.ValidationMessageFor(model => model.Cargo.NombreCargo, "", new { @class = "text-danger" })
    </div>
</div>
@Html.Hidden("IdTrabajador", Model.Id)