@model SVPDomain.Visitante

<div class="clearfix" style="margin-bottom: 0px;"></div>
<div class="form-group">
    <label class="col-sm-2 control-label">Nombre :</label>
    <div class="col-md-5">
        @Html.EditorFor(model => model.NombreCompleto, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
        @Html.ValidationMessageFor(model => model.NombreCompleto, "", new { @class = "text-danger" })
    </div>
</div>
<div class="clearfix" style="margin-bottom: 5px;"></div>
<div class="form-group">
    <label class="col-sm-2 control-label">Cargo :</label>
    <div class="col-md-5">
        @Html.EditorFor(model => model.Cargo, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
        @Html.ValidationMessageFor(model => model.Cargo, "", new { @class = "text-danger" })
    </div>
</div>
<div class="clearfix" style="margin-bottom: 5px;"></div>
<div class="form-group">
    <label class="col-sm-2 control-label">Institución :</label>
    <div class="col-md-5">
        @Html.EditorFor(model => model.Institución, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
        @Html.ValidationMessageFor(model => model.Institución, "", new { @class = "text-danger" })
    </div>
</div>
@Html.Hidden("IdVisitante", Model.Id)