@model SVPDomain.Apoderado

<div class="clearfix" style="margin-bottom: 0px;"></div>
<div class="form-group">
    <label class="col-sm-2 control-label">Apoderado :</label>
    <div class="col-md-5">
        @Html.EditorFor(model => model.NombreCompleto, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
        @Html.ValidationMessageFor(model => model.NombreCompleto, "", new { @class = "text-danger" })
    </div>
</div>
@Html.Hidden("IdApoderado", Model.Id)
